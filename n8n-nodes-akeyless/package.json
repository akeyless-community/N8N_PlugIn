{
  "name": "n8n-nodes-akeyless",
  "version": "1.0.1",
  "description": "Secure N8N node for Akeyless Secrets Management integration with API Key authentication",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "akeyless",
    "secrets",
    "secrets-management",
    "api-key",
    "security"
  ],
  "license": "MIT",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-akeyless.git"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AkeylessApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Akeyless/Akeyless.node.js"
    ]
  },
  "devDependencies": {
    "@types/elliptic": "^6.4.18",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-security": "^2.1.0",
    "n8n-workflow": "^1.17.0",
    "prettier": "^3.2.0",
    "typescript": "^5.4.0"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.60.0"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "scripts": {
    "build": "tsc && npm run copy:assets",
    "copy:assets": "mkdir -p dist/nodes/Akeyless && cp src/nodes/Akeyless/*.svg dist/nodes/Akeyless/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier src --write",
    "lint": "eslint src --ext .ts,.js",
    "test:security": "npm audit",
    "prepare": "npm run build"
  }
}
